<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ssverma.espressolab">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowTextActivity" />
        <activity android:name=".custommatcher.CofferPreparationActivity" />
        <activity android:name=".idlingresources.IdlingResSampleActivity" />
        <activity android:name=".intentstesting.ContactsActivity" />
        <activity android:name=".intentstesting.CallActivity" />
        <activity android:name=".recyclerview.SomeRecyclerActivity"></activity>
    </application>

</manifest>